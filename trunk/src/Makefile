pluginloader.o: pluginloader.cpp pluginloader.h \
		modelsviewplugin.h \
		mvglwidget.h

plugindialog.h: plugindialog.ui 
	$(UIC) plugindialog.ui -o plugindialog.h

plugindialog.cpp: plugindialog.h plugindialog.ui 
	$(UIC) plugindialog.ui -i plugindialog.h -o plugindialog.cpp

plugindialog.o: plugindialog.cpp plugindialog.ui.h \
		plugindialog.h

moc_form.o: moc_form.cpp  form.h mvglwidget.h \
		pluginloader.h \
		modelsviewplugin.h

moc_plugindialog.o: moc_plugindialog.cpp  plugindialog.h 

