#############################################################################
# Makefile for building: 3dmodelsview
# Generated by qmake (1.07a) (Qt 3.3.1) on: Fri May 20 21:39:27 2005
# Project:  3dmodelsview.pro
# Template: subdirs
# Command: $(QMAKE) -o Makefile 3dmodelsview.pro
#############################################################################

MAKEFILE =	Makefile
QMAKE    =	qmake
DEL_FILE =    rm -f
CHK_DIR_EXISTS= test -d
MKDIR    = mkdir -p
SUBTARGETS =	 \
		sub-src \
		sub-carofeplugin

first: all

all: Makefile $(SUBTARGETS)

src/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "src" || $(MKDIR) "src"
	cd src && $(QMAKE) src.pro -o $(MAKEFILE)
sub-src: src/$(MAKEFILE) FORCE
	cd src && $(MAKE) -f $(MAKEFILE)

carofeplugin/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "carofeplugin" || $(MKDIR) "carofeplugin"
	cd carofeplugin && $(QMAKE) carofeplugin.pro -o $(MAKEFILE)
sub-carofeplugin: carofeplugin/$(MAKEFILE) FORCE
	cd carofeplugin && $(MAKE) -f $(MAKEFILE)

Makefile: 3dmodelsview.pro  /usr/lib/qt3/mkspecs/default/qmake.conf 
	$(QMAKE) -o Makefile 3dmodelsview.pro
qmake: qmake_all
	@$(QMAKE) -o Makefile 3dmodelsview.pro

all: $(SUBTARGETS)
qmake_all: src/$(MAKEFILE) carofeplugin/$(MAKEFILE)
	 ( [ -d src ] && cd src ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
	 ( [ -d carofeplugin ] && cd carofeplugin ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
clean uicables mocables uiclean mocclean lexclean yaccclean : qmake_all FORCE
	 ( [ -d src ] && cd src ; $(MAKE) -f $(MAKEFILE) $@; ) || true
	 ( [ -d carofeplugin ] && cd carofeplugin ; $(MAKE) -f $(MAKEFILE) $@; ) || true
uninstall_subdirs: qmake_all FORCE
	 ( [ -d src ] && cd src ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
	 ( [ -d carofeplugin ] && cd carofeplugin ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
install_subdirs: qmake_all FORCE
	 ( [ -d src ] && cd src ; $(MAKE) -f $(MAKEFILE) install; ) || true
	 ( [ -d carofeplugin ] && cd carofeplugin ; $(MAKE) -f $(MAKEFILE) install; ) || true
distclean: qmake_all FORCE
	 ( [ -d src ] && cd src ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true
	 ( [ -d carofeplugin ] && cd carofeplugin ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true

install:  install_subdirs

uninstall:  uninstall_subdirs

FORCE:

